@article{Schrodt2010,
abstract = {This paper summarizes the current state-of-the-art for generating{\$}\backslash{\$}nhigh-volume, near-real-time event data using automated coding methods,{\$}\backslash{\$}nbased on recent efforts for the DARPA Integrated Crisis Early Warning{\$}\backslash{\$}nSystem (ICEWS) and NSF-funded research. The ICEWS work expanded by{\$}\backslash{\$}nmore than two orders of magnitude previous automated coding efforts,{\$}\backslash{\$}ncoding of about 26-million sentences generated from 8-million stories{\$}\backslash{\$}ncondensed from around 30 gigabytes of text. The actual coding took{\$}\backslash{\$}nsix minutes. The paper is largely a general "how-to" guide to the{\$}\backslash{\$}npragmatic challenges and solutions to various elements of the process{\$}\backslash{\$}nof generating event data using automated techniques. It also discusses{\$}\backslash{\$}na number of ways that this could be augmented with existing open-source{\$}\backslash{\$}nnatural language processing software to generate a third-generation{\$}\backslash{\$}nevent data coding system.},
author = {Schrodt, Philip A.},
