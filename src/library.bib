@article{Murali2020,
abstract = {Researchers have long been interested in the role that norms can play in governing agent actions in multi-agent systems. Much work has been done on formalising normative concepts from human society and adapting them for the government of open software systems, and on the simulation of normative processes in human and artificial societies. However, there has been comparatively little work on applying normative MAS mechanisms to understanding the norms in human society. This work investigates this issue in the context of international politics. Using the GDELT dataset, containing machine-encoded records of international events extracted from news reports, we extracted bilateral sequences of inter-country events and applied a Bayesian norm mining mechanism to identify norms that best explained the observed behaviour. A statistical evaluation showed that the normative model fitted the data significantly better than a probabilistic discrete event model.},
archivePrefix = {arXiv},
arxivId = {2003.14027},
author = {Murali, Rohit and Patnaik, Suravi and Cranefield, Stephen},
eprint = {2003.14027},
title = {{Mining International Political Norms from the GDELT Database}},
url = {http://arxiv.org/abs/2003.14027},
year = {2020}
}
@article{Wang2018,
abstract = {Political event data have long been used in the quantitative study of international politics. With the aim of providing a data basis for quantitative study, a new metadata model is proposed to evaluate medium's interest and political events' influence of news from the international mainstream media more conveniently and effectively, by tailoring and extending existing event data sets' metadata. And then a political event data set is constructed based this new metadata model and focuses on the objective statistics of the event data. Finally, the event, Trump's visit to China, is chosen as a case study to show how this metadata model and event data set is applied into practice.},
author = {Wang, Biao and Zhang, Yiwei and Wang, Ding},
doi = {10.1109/BDS/HPSC/IDS18.2018.00017},
isbn = {9781538643990},
journal = {Proceedings - 4th IEEE International Conference on Big Data Security on Cloud, BigDataSecurity 2018, 4th IEEE International Conference on High Performance and Smart Computing, HPSC 2018 and 3rd IEEE International Conference on Intelligent Data and Securit},
keywords = {GDELT,event data set,metadata model,political event data},
pages = {14--19},
title = {{Research on a New Metadata Model of Political Event Data Set}},
year = {2018}
}
